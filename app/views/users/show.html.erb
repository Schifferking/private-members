<h1><%= "#{@user.email}'s created events" %></h1>

<ul>
  <% @user.events.each do |event| %>
    <li>
      <%= link_to event.name, "/events/#{event.id}" %>
    </li>
  <% end %>
</ul>

<h2>Attended events</h2>

<ul>
  <% @user.attended_events.each do |event| %>
    <li>
      <%= link_to event.name, "/events/#{event.id}" %>
    </li>
  <% end %>
</ul>
